{
    "name": "anime-db/shmop",
    "license": "MIT",
    "type": "library",
    "description": "Shmop is a simple and small abstraction layer for shared memory manipulation using PHP",
    "keywords": ["php", "shmop"],
    "homepage": "http://github.com/anime-db/shmop",
    "authors": [
        {
            "name": "Peter Gribanov",
            "email": "info@peter-gribanov.ru"
        }
    ],
    "autoload": {
        "psr-4": {
            "AnimeDb\\Shmop\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AnimeDb\\Shmop\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.0",
        "ext-shmop": "*"
    },
    "require-dev" : {
        "phpunit/phpunit": "4.8.*",
        "scrutinizer/ocular": "~1.3"
    },
    "suggest": {
        "gpslab/shmop": "Shmop has moved to a new package name. The package you have installed is deprecated."
    }
}
